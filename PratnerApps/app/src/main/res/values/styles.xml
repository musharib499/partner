<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <!--controls the ‘normal’ state of components such as an unselected EditText, and unselected Checkboxes and RadioButtons-->
        <item name="colorControlNormal">@color/colorAccent</item>
        <!--controls the ripple coloring-->
        <item name="colorControlHighlight">@color/colorAccent</item>
        <item name="android:colorControlHighlight" tools:targetApi="21">@color/colorAccent</item>
        <!--overrides colorAccent as the activated or checked state for Checkboxes and RadioButtons-->
        <item name="colorControlActivated">@color/colorAccent</item>
        <!--<item name="navigationBarColor">@color/colorPrimary</item>-->
        <item name="android:colorPrimary" tools:targetApi="21">@color/colorPrimary</item>
        <item name="android:colorPrimaryDark" tools:targetApi="21">@color/colorPrimaryDark</item>
        <item name="android:colorButtonNormal" tools:targetApi="21">@color/colorAccent</item>
        <item name="background_color">background_color</item>

    </style>
    <style name="DialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent" tools:targetApi="21">@color/colorAccent</item>
        <item name="colorPrimaryDark" tools:targetApi="21">@color/colorPrimaryDark</item>
        <item name="colorPrimary" tools:targetApi="21">@color/colorPrimary</item>

    </style>

    <style name="AppCompatNoActionBarTheme" parent="@style/AppTheme">
        <item name="android:textColorHighlight">@color/white</item>
        <item name="android:textColorHint">@android:color/darker_gray</item>
        <item name="android:textColorSecondary">@color/black</item>
        <item name="android:textColorPrimary">@color/black</item>
        <item name="android:navigationBarColor" tools:targetApi="21">@android:color/black</item>
        <!--<item name="navigationBarColor">@color/colorPrimary</item>-->

    </style>

    <style name="AppTheme.FullScreen" parent="@style/AppTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsTranslucent">true</item>

    </style>

