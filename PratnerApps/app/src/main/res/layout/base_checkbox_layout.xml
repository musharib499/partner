<?xml version="1.0" encoding="utf-8"?>
<com.gaadi.pratnerapps.theme.DealerLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/parentLayoutHeightWidthMatchStyling"
    android:orientation="vertical">


    <com.gaadi.pratnerapps.theme.DealerTextView
        android:id="@+id/tvTitleCheckBoxFragment"
        style="@style/titleCheckBoxFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_large"
        tools:text="lakshay" />

    <include
        android:id="@+id/vLine"
        layout="@layout/border_view_match_width" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/rcvCheckBoxItems"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/vLine"
        android:layout_margin="@dimen/margin_medium" />

</com.gaadi.pratnerapps.theme.DealerLinearLayout>