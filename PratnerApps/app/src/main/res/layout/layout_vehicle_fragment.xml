<?xml version="1.0" encoding="utf-8"?><!--<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">-->

<com.gaadi.pratnerapps.theme.DealerLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/layoutHeightWrapWidthMatchStyling"
    android:gravity="center_vertical"
    android:orientation="horizontal">
    <com.gaadi.pratnerapps.theme.DealerLinearLayout
        style="@style/layoutHeightWidthWrapStyling"
        android:gravity="center_vertical"
        android:layout_margin="@dimen/margin_medium"
        android:orientation="horizontal">

        <com.gaadi.pratnerapps.theme.DealerImageView
            style="@style/ImageViewWithTintColor.HeadingColor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_place" />
        <com.gaadi.pratnerapps.theme.DealerTextView
            android:id="@+id/tvAddress"
            style="@style/TextStyleNormal.Large"
            tools:text="old delhi,New Delhi" />
    </com.gaadi.pratnerapps.theme.DealerLinearLayout>


    <com.gaadi.pratnerapps.theme.DealerTextView
        android:id="@+id/tvPrice"
        style="@style/TextStyleNormal.Large"
        android:text="@string/price" />
</com.gaadi.pratnerapps.theme.DealerLinearLayout><!--
</merge>-->
